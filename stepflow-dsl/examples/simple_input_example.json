{
  "name": "simple_task",
  "dsl": {
    "comment": "简单任务示例",
    "version": "1.0",
    "startAt": "MyTask",
    "states": {
      "MyTask": {
        "type": "task",
        "resource": "echo",
        "parameters": {
          "message": "Hello World"
        },
        "end": true
      }
    }
  }
} 